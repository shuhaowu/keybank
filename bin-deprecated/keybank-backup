#!/usr/bin/env python
from __future__ import absolute_import

import os

from keybank import get_keybank_from_commandline


def main():
  kb, args = get_keybank_from_commandline("backups all files specified by manifest.json")
  kb.backup_files(args.directory_on_machine, dry_run=args.dry_run)


if __name__ == "__main__":
  main()
